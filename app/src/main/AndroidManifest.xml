<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hnam.w9demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".sharedelement.DetailSharedElement" />
        <activity android:name=".sharedelement.MainSharedElementActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".transition.DetailActivity"
            android:theme="@style/AppTheme.DetailAppTheme" />
        <activity
            android:name=".transition.MainActivity"
            android:theme="@style/AppTheme.ActivityMain" />


        <activity android:name=".sharedelement.DetailSharedBActivity"
            android:theme="@style/AppTheme.DetailImage"/>
        <activity android:name=".propertyanimation.PropertyAnimationAct"
            android:screenOrientation="fullSensor"/>
    </application>

</manifest>